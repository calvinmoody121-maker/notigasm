{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "skipLibCheck": true,
    // "moduleResolution": "node16",
    // "customConditions": ["someCondition"],
    "jsx": "react-native",
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
